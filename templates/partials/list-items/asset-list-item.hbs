<tr class="asset-item" data-id="{{id}}">
    <td class="c2d10-contentbox asset">
        {{name}}
    </td>
    <td class="c2d10-contentbox asset">
        {{system.assetType}}
    </td>
    <td class="c2d10-contentbox asset">
        <div class="flex-row">
            {{#dots system.quality 5}}{{/dots}}
        </div>
    </td>
    <td class="c2d10-contentbox asset">
        <div class="flex-row">
            {{#dots system.value 5}}{{/dots}}
        </div>
    </td>
</tr>